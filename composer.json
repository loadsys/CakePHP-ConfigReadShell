{
  "name": "loadsys/cakephp-config-read",
  "description": "A CakePHP plugin that provides a Shell to read an app's Configure vars from the command line.",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "configure",
    "shell"
  ],
  "homepage": "https://github.com/loadsys/CakePHP-ConfigReadShell",
  "license": "MIT",
  "authors": [
    {
      "name": "Brian Porter",
      "email": "beporter@users.sourceforge.net"
    }
  ],
  "support": {
    "issues": "https://github.com/loadsys/CakePHP-ConfigReadShell/issues",
    "source": "https://github.com/loadsys/CakePHP-ConfigReadShell"
  },
  "require": {
    "php": ">=7.0",
    "cakephp/cakephp": "^3.6",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "loadsys/loadsys_codesniffer": "dev-master",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ConfigRead\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "vendor/cakephp/cakephp/test",
      "ConfigRead\\Test\\": "tests"
    }
  }
}
